<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="summit-management">
    <default-activity>__summit-management</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__1">
      <description>The id of the customer that should be diplayed when launching the summit management view.</description>
      <display-name>Customer ID</display-name>
      <name>customerId</name>
      <class>java.lang.Integer</class>
    </input-parameter-definition>
    <view id="__summit-management">
      <page>/fragments/summit-management.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
